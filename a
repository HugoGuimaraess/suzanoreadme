function dividir(a, b) {
  if (b === 0) {
    // Mensagem de erro personalizada
    throw new Error("Não é possível dividir por zero. Verifique o valor de 'b'.");
  }
  return a / b;
}






from image_processor import load_image, resize_image, apply_blur

img = load_image('foto.jpg')
img = resize_image(img, (300, 300))
img = apply_blur(img)
img.show()
